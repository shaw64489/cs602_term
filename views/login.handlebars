
    <div class="row login-form">
        <div class="eol-img"><img src="/images/login.png" alt="Dont Panic
            Error Image "></div>
        <div class="form-col col-md-6 col-md-offset-3 ">
            <form method="POST" action="/login">
                <div class="form-group">
                    <!--<label for="email ">Email</label>-->
                    <input class="form-control" type="text" id="email" placeholder="email" name="email">
                </div>
                <div class="form-group ">
                    <!--<label for="password ">Password</label>-->
                    <input class="form-control" type="password" id="password" placeholder="password" name="password">
                </div>
                <button class="btn btn-primary" type="submit">Log in </button>
                <a class="signup__link" href="/register">Haven't registered?</a>
            </form>
        </div>
    </div>
